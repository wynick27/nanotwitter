<% @chatgroup.messages.each do |m| %>
	<% if @curuser.id == m.user_id %>
	<%=erb(:chat_message_right,:locals=>{:m=>m})%>
	<% else %>
	<%=erb(:chat_message_left,:locals=>{:m=>m})%>
	<%end%>
<% end %>
<script>
$('.msg-moment').each(function(i, obj){
    if ($(obj).html().indexOf("UTC") > -1){
        interpretDate(obj);
    }
 });
</script>